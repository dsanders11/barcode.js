{
  "id": "barcodejs",
  "closure-library": "node_modules/google-closure-library/closure/goog/",
  "paths": [
    "src",
    "node_modules/google-closure-library/third_party/closure/goog/mochikit/"
  ],
  "test-template": "src/plovr-tests.soy",
  "debug": true,
  "mode": "WHITESPACE",
  "level": "VERBOSE",
  "output-charset": "UTF-8",
  "language-out": "ECMASCRIPT5_STRICT",
  "define": {
  },
  "externs": [
    "externs/iconv.js"
  ],
  "experimental-compiler-options": {
    "languageIn": "ECMASCRIPT6_STRICT",
    "generateExports": true,
    // These options degrades performance significantly, so disable them.
    "inlineFunctions": false,
    "collapseProperties": false // Flattens multi-level property names (e.g. a$b = x)
  },
  "checks": {
    "lintChecks": "WARNING",
    "missingProvide": "WARNING",
    "missingRequire": "WARNING",
    "missingReturn": "WARNING",
    "unusedLocalVariables": "WARNING",
    "extraRequire": "WARNING"
    // "reportUnknownTypes": "WARNING"
  }
}
